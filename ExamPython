def defTableauJoueur(tableauMotMystere1, tableauJoueur1):
    for i in range (1,7):
        tableauJoueur1[i-1] = input("Quelle mot ? (Lettre par lettre en minuscule)")
    return tableauJoueur1

def chercherLesLettres (tableauMotMystere1, tableauJoueur1):
    for i in range (1,7):
        for j in range (1,7):
            if (tableauJoueur[i-1] != tableauMotMystere1[j-1]):
                print ("non")
            else :
                print("oui")




tableauMotMystere = ['c','a','s','t','o','r']
tableauJoueur = ['_','_','_','_','_','_']
defTableauJoueur (tableauMotMystere, tableauJoueur)
chercherLesLettres (tableauMotMystere, tableauJoueur)
